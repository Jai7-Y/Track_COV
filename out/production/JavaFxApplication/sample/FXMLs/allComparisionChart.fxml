<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<ScrollPane prefHeight="800.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AllComparisionChart">
   <content>
      <AnchorPane prefHeight="800.0" prefWidth="1500.0">
         <children>
            <LineChart fx:id="lineChart" layoutX="2.0" layoutY="5.0" prefHeight="739.0" prefWidth="1450.0" title="COMPARISION">
              <xAxis>
                <CategoryAxis label="STATES" side="BOTTOM" fx:id="xAxis" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="yAxis" label="NUMBER OF CASES" side="LEFT" />
              </yAxis>
               <effect>
                  <DropShadow color="#2f00ff" />
               </effect>
            </LineChart>
            <Button fx:id="backBtnL" layoutX="712.0" layoutY="762.0" mnemonicParsing="false" onAction="#backListenerL" text="BACK">
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <effect>
                  <DropShadow color="#2f00ff" radius="11.9" width="28.6" />
               </effect>
            </Button>
         </children>
      </AnchorPane>
   </content>
</ScrollPane>

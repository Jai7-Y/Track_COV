<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.News">
   <children>
      <AnchorPane layoutY="-1.0" prefHeight="50.0" prefWidth="700.0">
         <children>
            <Label alignment="CENTER" layoutX="269.0" prefHeight="50.0" prefWidth="162.0" text="COVID NEWS" textFill="RED">
               <font>
                  <Font name="Algerian" size="26.0" />
               </font>
            </Label>
            <Button fx:id="refreshBtnN" layoutX="648.0" layoutY="24.0" mnemonicParsing="false" onAction="#refreshListenerN" text="Refresh">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
         </children>
      </AnchorPane>
      <Button fx:id="backBtnN" layoutX="324.0" layoutY="674.0" mnemonicParsing="false" onAction="#backListenerN" text="BACK">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>

<ScrollPane prefHeight="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ConfirmGraph">
   <content>
      <AnchorPane prefHeight="800.0" prefWidth="1500.0">
         <children>
            <BarChart fx:id="barChart" horizontalGridLinesVisible="false" layoutX="8.0" layoutY="8.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="739.0" prefWidth="1450.0" title="CONFIRMED CASES GRAPH">
              <xAxis>
                <CategoryAxis label="STATES" side="BOTTOM" tickLabelFill="#191818" fx:id="xAxis">
                     <effect>
                        <InnerShadow color="#c11111" />
                     </effect>
                  </CategoryAxis>
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="yAxis" label="NUMBER OF CASES" side="LEFT" />
              </yAxis>
               <effect>
                  <InnerShadow choke="0.26" color="#c11111" height="25.77" radius="14.767499999999998" width="35.3" />
               </effect>
            </BarChart>
            <Button fx:id="backBtnC" layoutX="718.0" layoutY="760.0" mnemonicParsing="false" onAction="#backListenerC" text="BACK" textFill="#c11111">
               <effect>
                  <InnerShadow color="#c11111" radius="13.575" width="35.3" />
               </effect>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </AnchorPane>
   </content>
</ScrollPane>

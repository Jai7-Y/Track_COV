<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Statewise">
   <children>
      <AnchorPane prefHeight="50.0" prefWidth="700.0">
         <children>
            <Label alignment="CENTER" layoutX="195.0" prefHeight="56.0" prefWidth="311.0" text="ALL STATE COVID STATS" textFill="#0008ff">
               <font>
                  <Font name="Algerian" size="26.0" />
               </font>
            </Label>
            <Button fx:id="refreshBtnS" layoutX="648.0" layoutY="30.0" mnemonicParsing="false" onAction="#refeshListenerS" text="Refresh">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
         </children>
      </AnchorPane>
      <Button fx:id="backBtnS" layoutX="324.0" layoutY="674.0" mnemonicParsing="false" onAction="#backListenerS" text="BACK">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <TableView fx:id="tbl" layoutY="107.0" prefHeight="565.0" prefWidth="704.0">
        <columns>
          <TableColumn fx:id="stateName_col" editable="false" prefWidth="358.0" text="STATES" />
          <TableColumn fx:id="confirmed_col" editable="false" prefWidth="89.0" text="CONFIRMED" />
            <TableColumn fx:id="active_col" editable="false" prefWidth="87.0" text="ACTIVE" />
            <TableColumn fx:id="recovered_col" editable="false" prefWidth="85.0" text="RECOVERED" />
            <TableColumn fx:id="deaths_col" editable="false" prefWidth="84.0" text="DEATHS" />
        </columns>
      </TableView>
      <TextField fx:id="searchBox" layoutX="14.0" layoutY="73.0" onKeyTyped="#searchListener" prefHeight="25.0" prefWidth="251.0" promptText="search state name">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
